{
  "name": "songqueue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma migrate deploy && prisma generate && next build",
    "start": "next start",
    "fix": "pnpm format --log-level silent && pnpm lint --fix",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx|json)\"",
    "prepare": "cd .. && husky install web/.husky",
    "spinup": "pnpm install && pnpm docker && pnpm pdev",
    "docker": "docker compose up --detach",
    "docker:kill": "(docker ps -aq | xargs docker stop | xargs docker rm) && sudo rm -r .s3local",
    "pdev": "prisma migrate dev",
    "pgen": "prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/s3-presigned-post": "^3.485.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.5",
    "@prisma/client": "5.8.0",
    "@tanstack/react-query": "^5.17.15",
    "@tanstack/react-query-devtools": "^5.17.18",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "form-data": "^4.0.0",
    "framer-motion": "^10.18.0",
    "mailgun.js": "^9.4.1",
    "next": "14.0.4",
    "next-auth": "5.0.0-beta.4",
    "react": "^18",
    "react-dom": "^18",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prisma": "^5.8.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
